{
  "expo": {
    "name": "Mova",
    "slug": "mova",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mova.png",
    "userInterfaceStyle": "light",
    "scheme": "mova",
    "splash": {
      "image": "./assets/mova.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mova.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.davidmedev.mova",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/mova.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ec81bd47-1931-4a1f-9e90-61db0de2e92c"
      }
    }
  }
}
